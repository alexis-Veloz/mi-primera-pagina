<! DOCTYPE html>
< html  lang = " en " >
  < cabeza >
    < meta  charset = " UTF-8 " >
    < título > Página de contacto </ título >
    <% incluye parciales / cabeza %>
  </ cabeza >
  < cuerpo >
    <% incluye parciales / navegación %>

    < div  class = " contenedor p-5 " >
      < div  class = " row " >
        < div  class = " col-sm-4 offset-md-4 " >
          < div  class = " tarjeta animada flipInY " >
            < div  class = " card-header bg-dark text-white text-center " >
              < h3 > Contáctame </ h3 >
            </ div >
            < div  class = " card-body " >
              < form  action = " " >
                < div  class = " form-group " >
                  < input  class = " form-control "  type = " text "  placeholder = " Email " >
                </ div >
                < div  class = " form-group " >
                  < textarea  class = " form-control "  name = " "  cols = " 30 "  rows = " 10 "  placeholder = " Message " > </ textarea >
                </ div >
                < div  class = " form-group " >
                  < button  class = " btn btn-primary btn-block " >
                    Salvar
                  </ button >
                </ div >
              </ form >    
            </ div >
          </ div >
        </ div >
      </ div >
    </ div >

    <% incluye parciales / pie de página %>
  </ cuerpo >
</ html >